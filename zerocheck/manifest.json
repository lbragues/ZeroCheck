{
  "name": "zeroCheck",
  "version": "1.1",
  "description": "checks if a zerobin replica is safe.",
  "background": { "page": "background.html" },
  "page_action" :
  {
    "default_icon" : "icon-48.png",
    "default_title" : "Check Results",
    "default_popup": "popup.html"
  },
  "permissions" : [
    "tabs","http://*/*","https://*/*","http://sizeone.net/zerobin/list.json"
  ],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png",
    "256" : "icon-256.png"
  },
  "manifest_version": 1
}
